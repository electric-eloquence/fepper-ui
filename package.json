    "@babel/runtime": "^7.5.4",
    "@wdio/cli": "5.13.2",
    "@wdio/local-runner": "5.13.2",
    "@wdio/mocha-framework": "5.13.2",
    "@wdio/spec-reporter": "5.13.2",
    "@wdio/sync": "5.13.2",
    "babel-eslint": "^10.0.2",
    "chai": "4.x",
